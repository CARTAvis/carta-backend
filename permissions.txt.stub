# some very basic folder-based permissions for images in the viewer
# each user has their own API key which they can enter into the viewer through
# a debug menu. The key will be stored in the browser's local storage
/:
*
public:
*
angus:
    [angus_api_key]
angus/shared:
    [angus_api_key]
    [russ_api_key]
angus/public:
    *
russ:
    [russ_api_key]
russ/public:
    *

